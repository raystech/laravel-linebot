{
    "name": "raystech/laravel-linebot",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Piyapan Rodkuen",
            "email": "piyapannr@gmail.com",
            "homepage": "https://www.raystech.github.io"
        }
    ],
    "homepage": "https://github.com/raystech/laravel-linebot",
    "keywords": ["Laravel", "laravel-linebot", 'linebot'],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Raysctech\\Linebot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raysctech\\Linebot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raysctech\\Linebot\\laravel-linebotServiceProvider"
            ],
            "aliases": {
                "laravel-linebot": "Raysctech\\Linebot\\Facades\\laravel-linebot"
            }
        }
    }
}